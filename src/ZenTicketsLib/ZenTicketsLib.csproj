<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net7.0</TargetFramework>
        <PackageId>ZenTicketsLib</PackageId>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <AssemblyName>ZenTicketsLib</AssemblyName>
        <RootNamespace>ZenTicketsLib</RootNamespace>
        <Version>1.0.0</Version>
        <Authors>Jorge Herrera Ulloa</Authors>
        <Description>A .NET library for zendesk search export api endpoint consumption.</Description>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
        <PackageTags>zendesk client</PackageTags>

    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.Extensions.Options" Version="7.0.1" />
    </ItemGroup>

</Project>
